# Environment configuration for Travel A2A Backend (Improved)

# API Mode - Set to 1 for Vertex AI (ADK) mode, 0 for Direct API mode
GOOGLE_GENAI_USE_VERTEXAI=0

# Model to use
GOOGLE_GENAI_MODEL=gemini-2.0-flash

# API Key for Direct API mode (required if GOOGLE_GENAI_USE_VERTEXAI=0)
GOOGLE_API_KEY=your_google_api_key_here

# Tavily API Key (required for Tavily search functionality)
TAVILY_API_KEY=your_tavily_api_key_here

# Port for the FastAPI server (default: 8000)
PORT=8000
