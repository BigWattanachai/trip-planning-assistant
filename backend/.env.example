# Google API credentials
GOOGLE_API_KEY=your_google_api_key_here

# Choose Model Backend: 0 -> Direct API, 1 -> Vertex AI (ADK)
GOOGLE_GENAI_USE_VERTEXAI=0

# Model to use
GOOGLE_GENAI_MODEL=gemini-2.0-flash

# YouTube API key (can be the same as Google API key with YouTube Data API v3 enabled)
YOUTUBE_API_KEY=your_youtube_api_key_here

# Vertex AI Project settings (only needed if GOOGLE_GENAI_USE_VERTEXAI=1)
GOOGLE_CLOUD_PROJECT=your_gcp_project_id_here
VERTEX_LOCATION=us-central1

# Server settings
PORT=8000
HOST=0.0.0.0

# Tavily API Key for search functionality
TAVILY_API_KEY=your_tavily_api_key_here
